{
  "manifest_version": 3,
  "name": "CHUB copy extension",
  "version": "1.0.0",
  "description": "A basic example extension with only required keys",
  "icons": {
    "32": "icon-32.png"
  },
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["scripting"],
  "host_permissions": ["http://*/*", "https://*/*"]
}
